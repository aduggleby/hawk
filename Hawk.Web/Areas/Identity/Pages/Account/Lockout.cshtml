@page
@model LockoutModel

@{
    ViewData["Title"] = "Locked out";
}

<div class="mx-auto max-w-xl py-10">
    <div class="hawk-card border-[color:var(--text-primary)] border-dashed">
        <div class="text-xs font-mono uppercase tracking-tight text-zinc-500">Auth</div>
        <h1 class="text-2xl font-semibold tracking-tight">Locked out</h1>
        <p class="text-sm text-zinc-600 mt-2">This account is temporarily locked. Please try again later.</p>

        <div class="mt-6">
            <a class="hawk-btn hawk-btn-ghost" asp-page="./Login">Back</a>
        </div>
    </div>
</div>

